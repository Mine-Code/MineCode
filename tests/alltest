     j = 0
     while j < i:
      if j == 0:
        print("*", end="")
                     
      else:
        print(" ", end="")
      j++
  
  a = 1
  a *= 2 ^ 2 * 2 + 2 < 2 == ++a ^ 3 * 4 >> a++ | 2 - 2 < 2 ^ 2 * 2 + 2 + (2 * 2 ^ 2 + 2 << 2 | 2)

